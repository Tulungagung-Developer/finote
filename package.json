{
  "name": "finote-backend",
  "version": "0.0.0-dev",
  "description": "",
  "author": "haloivanid <ivanazis.id@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --log-level silent",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky",
    "typeorm": "ts-node -r tsconfig-paths/register --files ./node_modules/typeorm/cli.js",
    "migration:generate": "wrap () { yarn typeorm migration:generate -d src/library/typeorm/datasource.typeorm.ts src/databases/migrations/\"$*\" && npm run format | cat; }; wrap",
    "migration:run": "yarn typeorm -d src/library/typeorm/datasource.typeorm.ts migration:run",
    "migration:revert": "yarn typeorm -d src/library/typeorm/datasource.typeorm.ts migration:revert",
    "seed:generate": "wrap () { yarn typeorm migration:create src/databases/seeds/\"$*\" && npm run format | cat; }; wrap",
    "seed:run": "yarn typeorm -d src/library/typeorm/datasource-seed.typeorm.ts migration:run",
    "seed:revert": "yarn typeorm -d src/library/typeorm/datasource-seed.typeorm.ts migration:revert",
    "erdia": "TS_NODE_PROJECT=tsconfig.json node -r ts-node/register -r tsconfig-paths/register ./node_modules/erdia/dist/cjs/cli.cjs",
    "erd:build": "npx rimraf .erd && mkdir .erd && yarn erdia build"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/platform-fastify": "^10.4.1",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/nestjs": "^8.25.0",
    "@sentry/profiling-node": "^8.25.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "deepmerge-ts": "^7.1.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "nestjs-pino": "^4.1.0",
    "pg": "^8.12.0",
    "pino-http": "^10.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.10",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "erdia": "^3.7.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.4",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@ctrl/(.*)": "<rootDir>/controllers/$1",
      "@db/(.*)": "<rootDir>/databases/$1",
      "@libs/(.*)": "<rootDir>/library/$1",
      "@core/(.*)": "<rootDir>/core/$1",
      "@conf/(.*)": "<rootDir>/configs/$1"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
